<h1>New Merchant:</h1>

<%= form_tag "/merchants", method: :post do %>
  <%= label_tag :name %>
    <br>
  <%= text_field_tag :name %>
    <br>
  <%= label_tag :address %>
    <br>
  <%= text_field_tag :address %>
    <br>
  <%= label_tag :city %>
    <br>
  <%= text_field_tag :city %>
    <br>
  <%= label_tag :state %>
    <br>
  <%= text_field_tag :state %>
    <br>
  <%= label_tag :zip %>
    <br>
  <%= text_field_tag :zip %>
    <br><br>
    <%= submit_tag "Create" %>
<% end %>
